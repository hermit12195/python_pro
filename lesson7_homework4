"""Custom module to test the doctest module"""
import doctest
from math import factorial


def is_even(n: int) -> bool:
    """
    Tests if the number is even.
    >>> is_even(4)
    True
    >>> is_even(5)
    False
    """
    return n % 2 == 0


def get_factorial(n: int) -> int:
    """
    Returns the factorial of the number.
    :param n: int
    :return: int
    >>> factorial(5)
    120
    """
    return factorial(n)
